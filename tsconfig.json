{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/pages/*": ["pages/*"],
      "@/app/*": ["app/*"],
      "@/lib/*": ["lib/*"],
      "@/styles/*": ["styles/*"],
      "assets/*": [
        "src/assets/*"
      ],
      "components": [
        "src/components"
      ],
      "components/*": [
        "src/components/*"
      ],
      "data/*": [
        "src/data/*"
      ],
      "database/*": [
        "src/database/*"
      ],
      "helpers/*": [
        "src/helpers/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "layouts/*": [
        "src/layouts/*"
      ],
      "lib/*": [
        "src/lib/*"
      ],
      "pages/*": [
        "src/pagesOLD/*"
      ],
      "providers/*": [
        "src/providers/*"
      ],
      "store/*": [
        "src/store/*"
      ],
      "styles/*": [
        "src/styles/*"
      ],
      "types": [
        "src/types"
      ],
      "types/*": [
        "src/types/*"
      ],
      "utils/*": [
        "src/utils/*"
      ]
    },
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-auth.d.ts", "next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
